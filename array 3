<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<style>
    #red{
        color: red;
    }
</style>
<body>
    <p id="out-1"></p>
    <script>
        function stampaArray(arr){
            for(let i = 0;i<arr.length;i++){
                if(isMultipleof(arr[i],7)){
                    document.write(`<p id="red"> ${arr[i]} </p>`)
                }else{
                    document.write(`<p> ${arr[i]} </p>`)
                }
            }
        }

        function isMultipleof(n,m){
            // n[int] : controllo che sia multiplo di n
            // m
            if(n % m === 0){
                return true
            }
            return false
        }
        console.log(isMultipleof(6,7))
        console.log(isMultipleof(14,7))
        console.log(isMultipleof(25,5))
       
       
        let numeri = []

        for(let i = 0;i <1000;i++){
            numeri[i] = i+1
            console.log(numeri)
        }

        stampaArray(numeri);
    </script>
</body>
</html>
